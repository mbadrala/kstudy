{
  "name": "K Study",
  "description": "Look up korean words on mouse hover",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://*/*", "http://*/*"]
    }
  ]
}
